{
    "name": "grafite/missioncontrol",
    "description": "The reporting package for Grafite Mission Control",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Lantz",
            "email": "matt@grafite.ca"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "mashape/unirest-php": "^3.0",
        "kassner/log-parser": "^1.4",
        "nesbot/carbon": "^1.32",
        "tightenco/collect": "^5.6"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "mikey179/vfsStream": "^1.6"
    },
    "bin": [
        "bin/atlas",
        "bin/xray"
    ],
    "autoload": {
        "psr-4": {
            "Grafite\\MissionControl\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "stable"
}
